                <item>
                    <uuid>6c0c54f069e84b3db2ad1dc9fc81ea18</uuid>
                    <name>Drive Fault State Active</name>
                    <type>DEPENDENT</type>
                    <key>drive.fault.state</key>
                    <delay>0</delay>
                    <history>7d</history>
                    <trends>30d</trends>
                    <units>status</units>
                    <description>Napęd w stanie awarii (Bit 3 słowa stanu napędu 3240 - FLT). 1=Awaria, 0=Brak awarii.</description>
                    <preprocessing>
                        <step>
                            <type>JAVASCRIPT</type>
                            <parameters>
                                <parameter>return (value &amp; (1 &lt;&lt; 3)) ? 1 : 0;</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>modbus.get[{HOST.HOST},0,3,3240,1,uint16]</key>
                    </master_item>
                    <triggers>
                        <trigger>
                            <uuid>c3fa4621934b4c879805ebe649fdfaf4</uuid>
                            <expression>last(/Modbus monitoring_mp/drive.fault.state)=1</expression>
                            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                            <recovery_expression>last(/Modbus monitoring_mp/drive.fault.state)=0</recovery_expression>
                            <name>{HOST.NAME} - DRIVE IS IN FAULT STATE!</name>
                            <priority>DISASTER</priority>
                            <description>Ten trigger wyzwala się, jeśli napęd znajduje się w stanie awarii (FLT bit stanu napędu jest 1).</description>
                        </trigger>
                    </triggers>
                </item>
